{
  "name": "nest-demo",
  "version": "0.0.0",
  "description": "",
  "author": "Plasticine-Yang",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "env:start": "docker compose -f ./docker-compose.dev.yaml up -d",
    "env:stop": "docker compose -f ./docker-compose.dev.yaml down",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier -w",
    "format:src": "prettier -w src",
    "test": "vitest",
    "test:u": "vitest -u",
    "commit": "cz",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mapped-types": "^1.2.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "0.13.0",
    "error-stack-parser": "^2.1.4",
    "joi": "^17.7.0",
    "mysql2": "2.2.5",
    "nestjs-pino": "^3.1.2",
    "pino-http": "^8.3.1",
    "pino-pretty": "^9.1.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "sonic-boom": "^3.2.1",
    "typeorm": "^0.3.11",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@plasticine-yang/eslint-config-typescript": "^0.1.5",
    "@types/express": "^4.17.15",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "commitizen": "^4.2.6",
    "cz-git": "^1.4.1",
    "eslint": "^8.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4",
    "vitest": "^0.28.1",
    "webpack": "^5.75.0"
  }
}
